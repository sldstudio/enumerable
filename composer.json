{
  "name": "solid/foundation",
  "description": "Shareable classes and traits useful for any PHP project",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "tools", "foundation", "enum", "base"
  ],
  "authors": [
    {
      "name": "Ruslan Gataullin",
      "email": "ruslan@solid.studio"
    }
  ],
  "autoload": {
    "psr-4": {
      "Solid\\Foundation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1.3",
    "ext-reflection": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "scripts": {
    "test": "php vendor/bin/phpunit --bootstrap vendor/autoload.php --configuration phpunit.xml tests"
  }
}
